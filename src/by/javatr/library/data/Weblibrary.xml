        <book id="3" title="Computer Science" author="No one" publishing_date="2014" price="25" source="www.oz.by"/>
        <book id="4" title="Stupid" author="Dostoevskiy" publishing_date="1956" price="38" source="www.oz.by"/>
        <book id="5" title="Old man and sea" author="Hamingueu" publishing_date="1990" price="40" source="www.oz.by"/>
        <book id="6" title="Hole Java" author="Gerbert Shild" publishing_date="2016" price="15" source="www.oz.by"/>
